            android:label="@string/title_activity_admin_add_work_type"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <activity
            android:name="com.example.dentallab.ExpandableDoctorListView"
            android:label="@string/title_activity_expandable_doctor_list_view"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
