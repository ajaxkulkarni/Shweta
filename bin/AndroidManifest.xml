        <activity
            android:name="com.example.dentallab.AdminAddWorkTypeActivity"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <activity
            android:name="com.example.dentallab.JobEntry"
            android:label="@string/title_activity_job_entry"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <activity
            android:name="com.example.dentallab.JobList"
            android:label="@string/title_activity_job_list"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
        </activity>
        <activity
            android:name="com.example.dentallab.BalanceSheet"
            android:label="@string/title_activity_balance_sheet" >
        </activity>
    </application>

</manifest>